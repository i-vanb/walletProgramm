{
  "expo": {
    "name": "Holey Pocket",
    "slug": "holey-pocket",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/newIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEBD00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.van-b.holey-pocket",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.van_b.holey_pocket",
      "versionCode": 1,
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
